<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/signatories_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/signatories"
        android:textStyle="bold"
        android:layout_marginTop="@dimen/spacing_double"
        android:textColor="@color/colorPrimary"
        android:textAppearance="@style/PaymentScreens.TextAppearance.StandardItem"
        tools:ignore="UnusedAttribute,NewApi" />

    <LinearLayout
        android:id="@+id/signatory_layout_01"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:visibility="gone"
        android:layout_marginTop="@dimen/spacing_double"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/bullet_textview_01"
            android:layout_width="wrap_content"
            android:layout_weight="0"
            android:layout_height="wrap_content"
            android:elevation="@dimen/elevation_header"
            tools:ignore="unusedattribute,newapi" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/bullet_address_edittext_01"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:minWidth="@dimen/spacing_huge"
            android:layout_height="wrap_content"
            android:imeOptions="actionDone"
            android:hint="@string/one"
            android:enabled="true"
            android:inputType="numberDecimal"
            android:focusableInTouchMode="false"
            android:clickable="false"
            android:longClickable="false"
            android:focusable="false"
            android:textColorHint="@color/tz_placeholder"
            android:textSize="@dimen/standard_item_text_size" />
    </LinearLayout>

